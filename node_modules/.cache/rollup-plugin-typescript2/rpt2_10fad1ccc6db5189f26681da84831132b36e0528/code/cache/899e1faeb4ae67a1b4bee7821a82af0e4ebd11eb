nst saltsAndSolvents = templates.reduce((acc, { struct, props }) => {\r\n            struct.abbreviation = String(props.abbreviation);\r\n            acc.push(struct);\r\n            return acc;\r\n        }, []);\r\n        saltsAndSolventsProvider.setSaltsAndSolventsList(saltsAndSolvents);\r\n        functionalGroupsProvider.addToFunctionalGroupsList(saltsAndSolvents);\r\n        dispatch(initSaltsAndSolvents(templates));\r\n        dispatch(appUpdate({ saltsAndSolvents: true }));\r\n    };\r\n}\r\nexport default saltsAndSolventsReducer;\r\n//# sourceMappingURL=index.js.map","references":["/home/ubuntu/work/4life/ketcher/node_modules/redux/index.d.ts","/home/ubuntu/work/4life/ketcher/packages/ketcher-react/src/script/ui/state/options/index.js","/home/ubuntu/work/4life/ketcher/packages/ketcher-core/dist/index.d.ts","/home/ubuntu/work/4life/ketcher/packages/ketcher-react/src/script/ui/state/templates/init-lib.js"],"map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../../src/script/ui/state/saltsAndSolvents/index.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;;;;;;;;6EAc6E;AAG7E,OAAO,EAAE,SAAS,EAAE,MAAM,YAAY,CAAA;AACtC,OAAO,EACL,wBAAwB,EACxB,wBAAwB,EAExB,aAAa,EAEd,MAAM,cAAc,CAAA;AACrB,OAAO,EAAE,cAAc,EAAE,MAAM,uBAAuB,CAAA;AAOtD,MAAM,YAAY,GAA0B;IAC1C,GAAG,EAAE,EAAE;IACP,IAAI,EAAE,IAAI;CACX,CAAA;AAED,MAAM,uBAAuB,GAAG,CAC9B,KAAK,GAAG,YAAY,EACpB,EAAE,IAAI,EAAE,OAAO,EAAa,EAC5B,EAAE;IACF,QAAQ,IAAI,EAAE;QACZ,KAAK,yBAAyB;YAC5B,OAAO,EAAE,GAAG,KAAK,EAAE,GAAG,OAAO,EAAE,CAAA;QAEjC;YACE,OAAO,KAAK,CAAA;KACf;AACH,CAAC,CAAA;AAED,MAAM,oBAAoB,GAAG,CAAC,GAAc,EAAE,EAAE,CAAC,CAAC;IAChD,IAAI,EAAE,yBAAyB;IAC/B,OAAO,EAAE,EAAE,GAAG,EAAE;CACjB,CAAC,CAAA;AAEF,MAAM,UAAU,6BAA6B,CAAC,OAAe;IAC3D,OAAO,KAAK,EAAE,QAAQ,EAAE,EAAE;QACxB,MAAM,QAAQ,GAAG,wBAAwB,CAAA;QACzC,MAAM,GAAG,GAAG,GAAG,OAAO,cAAc,QAAQ,EAAE,CAAA;QAC9C,MAAM,wBAAwB,GAAG,wBAAwB,CAAC,WAAW,EAAE,CAAA;QACvE,MAAM,wBAAwB,GAAG,wBAAwB,CAAC,WAAW,EAAE,CAAA;QACvE,MAAM,aAAa,GAAG,IAAI,aAAa,EAAE,CAAA;QACzC,MAA